public class Main
{
	public static void main(String[] args) {
		int a = 30;
		int b = 20;
		int c = 25;
		
		if(a<b){
		   System.out.println("1"); 
		}
		else if(b>c){
		    System.out.println("2");
		}
		else{
		    System.out.println("3");
		}
		
		int l = 150;
		int m = 200;
		int n = 100;
		
		int r = (l<n) ? 1 : 2;
		System.out.println(r);

        int s = 20;
        String t = (s < 18) ? "Lesser" : "Greater";
        System.out.println(t);
	}
}
