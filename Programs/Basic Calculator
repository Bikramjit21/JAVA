public class Main
{
    static void calculator(int a, int b, String r){
        if(r=="+"){
            System.out.println(a+b);
        }
        else if(r=="-"){
            System.out.println(a-b);
        }
        else if(r=="*"){
            System.out.println(a*b);
        }
        else if(r=="/"){
            System.out.println(a/b);
        }
    }
	public static void main(String[] args) {
	    calculator(12,3,"/");
	    
	}
}
