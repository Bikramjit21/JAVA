import java.io.*;
import java.util.*;

public class Main
{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		printIncreaseDecrease(n);
		
	}
	
	public static void printIncreaseDecrease(int n){
	    if(n==0){
	        return;
	    }
	    System.out.println(n);
	    printIncreaseDecrease(n-1);
	    System.out.println(n);
	}
}
